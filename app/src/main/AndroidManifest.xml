<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.spg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Spg">
        <activity android:name=".Tabbarmian"></activity>
        <activity android:name="module.Sy.Sy"
            tools:ignore="Instantiatable" />
        <activity android:name="module.Mine.Mine"
            tools:ignore="Instantiatable,MissingClass" />
        <activity android:name="module.Gwc.Gwc"
            tools:ignore="Instantiatable" />
        <activity android:name="module.Fenlei.Fenlei"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>